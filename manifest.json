{
  "name": "8trhacker",
  "version": "17.3",
  "description": "Make Spotify playlists from 8tracks playlists",
  "content_scripts": [
    {
      "matches": ["http://8tracks.com/*"],
      "js": ["jquery-1.8.2.min.js","8Trhacker.js"]
    }
  ],
  "web_accessible_resources": [
    "icon.png"
  ],
  "manifest_version": 2
}